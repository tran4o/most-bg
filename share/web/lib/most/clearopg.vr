var cnt = 1;

console.log("delete OPG entries");

for (var s: db.most.opg_entry.SELECT() ) {
    s.delete();
    s.commit();
    if ( ( cnt++ %128 ) == 0) {
        console.log("deleted " + cnt + " records...");
        db.commit();
    }
}

